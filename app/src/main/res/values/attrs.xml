<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FocusView">
        <attr name="VisibleRow" format="integer"></attr>
        <attr name="VisibleCol" format="integer"></attr>
        <attr name="Gap" format="integer"></attr>
        <attr name="Oriental">
            <enum name="Horizontal" value="1"></enum>
            <enum name="Vertical" value="0"></enum>
        </attr>
        <attr name="BackgroundColor" format="color"></attr>
    </declare-styleable>
    <declare-styleable name="AutoLoadImageView">
        <attr name="preLoadImage" format="reference"/>
        <attr name="errorImage" format="reference"/>
    </declare-styleable>

    <declare-styleable name="RainbowAnimView">
        <attr name="rainbowWidth" format="dimension"/>
        <attr name="rainbowHeight" format="dimension"/>
        <attr name="rainbow" format="reference"/>
        <attr name="rainbowLeftMargin" format="dimension"/>

    </declare-styleable>
</resources>